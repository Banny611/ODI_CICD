void invokeRESTReverse(operation, requestTemplateName, tableName, responseOutputStream) {
         
        
    def restCall = new InternalInvokeRESTfulService();
    odiRef.configureTool(restCall);
    
    restCall.setContext("<% out.print(bicsContext); %>");
    restCall.setLSchema("<% out.print(logicalSchema); %>");
    restCall.setOperation(operation);
    
    restCall.setResponseStream(responseOutputStream);
    
    def restParameters = new Vector();
    restParameters.add(["-HEADER.X-ID-TENANT-NAME", "<% out.print(bicsDomain); %>"] as String[]);
    
    if (!"".equals(requestTemplateName) && !"".equals(tableName))
        restParameters.add(["-REQUEST_TEMPLATE." + requestTemplateName, tableName] as String[]);            
    
    restCall.setParameters(restParameters);
    
    restCall.execute(); 
}


void logInfo (String message) {
    odiRef.getLogger().log(java.util.logging.Level.INFO, "[Reverse]" + message);
}

interface Persistable {
    void persist(statement)
}

trait Updatable implements Persistable {
    void persist(statement) {
        statement.executeUpdate();
    }
}

trait Clearable implements Persistable {
    void persist(statement) {
        statement.clearParameters();
        super.persist(statement);
    }
}

trait ContainerSettable implements Persistable {
    void persist(statement) {
        statement.setString(1, ownerModel);
        statement.setString(2, containerName);
        statement.setString(3, containerName);
        statement.setString(4, containerAlias);
        statement.setString(5, remarks);
        statement.setString(6, "1");
        statement.setString(7, containerType);
        super.persist(statement);
    }
}

trait ColumnSettable implements Persistable {
    void persist(statement) {
        statement.setString(1, ownerModel);
        statement.setString(2, ownerTable);
        statement.setString(3, columnName);
        statement.setString(4, columnHeading);     
        statement.setString(5, columnRemarks);
        statement.setString(6, columnDataTypeName);
        if(columnPosition != null)
            statement.setInt(7, columnPosition);
        else
            statement.setNull(7, Types.INTEGER);
        if(columnDataTypeLength != null)
            statement.setInt(8, columnDataTypeLength);
        else
            statement.setNull(8, Types.INTEGER);
        if(columnDataTypePrecision != null)
            statement.setInt(9, columnDataTypePrecision);  //precision
        else
            statement.setNull(9, Types.INTEGER);
        if(columnDataTypeLength != null)
            statement.setInt(10, columnDataTypeLength);
        else
            statement.setNull(10, Types.INTEGER);
      
        if (!isColumnNullable) 
            statement.setInt(11, 1);
        else
            statement.setInt(11, 0);
        
        statement.setString(12, "1");
        statement.setString(13, "1");
        statement.setString(14, columnDescription);
        statement.setString(15, columnDefaultValue);
        super.persist(statement);
    }
}

trait IndexSettable implements Persistable {
    void persist(statement) {
        statement.setString(1, ownerModel);
        statement.setString(2, tableName);
        statement.setString(3, indexName);
        statement.setString(4, indexType);
        super.persist(statement);
    }
}

trait IndexColumnSettable implements Persistable {
    void persist(statement) {
        statement.setString(1, ownerModel);
        statement.setString(2, tableName);
        statement.setString(3, indexName);
        statement.setString(4, indexColumnName);
        statement.setInt(5, indexColPosition);
        super.persist(statement);
    }
}

class BicsContainer implements Updatable, ContainerSettable, Clearable {
    def ownerModel;
    def containerName;
    def containerAlias;
    def containerType = "T";
    def remarks = "";
}

class BicsColumn implements Updatable, ColumnSettable, Clearable {
    def ownerModel;
    def ownerTable;
    def columnName;
    def columnDataTypeName;
    def columnDataTypeLength;
    def columnDataTypePrecision;
    def columnPosition;
    def columnHeading;
    def isColumnNullable = true;
    def columnRemarks = "";
    def columnDescription = "";
    def columnDefaultValue;
}

class BicsIndex implements Updatable, IndexSettable, Clearable {
    def ownerModel;
    def tableName;
    def indexName;
    def indexType;
}

class BicsIndexColumn implements Updatable, IndexColumnSettable, Clearable {
    def ownerModel;
    def tableName;
    def indexName;
    def indexColumnName;
    def indexColPosition;
}

void fillContainer(modelRef, tableName, removeAliasChars, ps) {
    def container              = new BicsContainer();
    container.ownerModel       = modelRef;
    container.containerName    = tableName;

    container.containerAlias = container.containerName.replace('.', '_');
    if (removeAliasChars != null)
      container.containerAlias = container.containerAlias.replace(removeAliasChars, "");
    
    container.containerType    = "T";
    container.remarks          = "";
    container.persist(ps);
}

void fillColumnInfo(modelRef, tableName, obj, colPosition, isReverseTables, psColumns) {
    def columnObj                     = new BicsColumn();
    columnObj.columnName              = obj.getString("columnName");
    columnObj.ownerModel              = modelRef;
    columnObj.ownerTable              = tableName;
    columnObj.columnDataTypeName      = obj.getString("dataType");
    if("DECIMAL".equalsIgnoreCase(columnObj.columnDataTypeName) || "INTEGER".equalsIgnoreCase(columnObj.columnDataTypeName))
        columnObj.columnDataTypeName  = "NUMBER";
    columnObj.columnDataTypeLength    = obj.getString("length").isInteger() ? obj.getInt("length") : null;
    columnObj.columnDataTypePrecision = obj.getString("precision").isInteger() ? obj.getInt("precision") : null;
    columnObj.columnHeading           = columnObj.columnName.length() > 35? 
                                            columnObj.columnName.substring(0, 35) : columnObj.columnName;
    if(isReverseTables) {
        columnObj.isColumnNullable    = obj.getBoolean("nullable");
        columnObj.columnPosition      = colPosition;
        columnObj.columnDefaultValue  = obj.getString("defaultValue");
    } else {
        columnObj.columnPosition      = obj.getString("position").isInteger() ? obj.getInt("position") : null;
    }
    columnObj.persist(psColumns);
}

void fillIndexInfo(ownerModel, tableName, indexDef, psIndexes) {
    def indexObj           = new BicsIndex();
    indexObj.ownerModel    = ownerModel;
    indexObj.tableName     = tableName;
    indexObj.indexName     = indexDef.getString("indexName");
    def isUnique           = indexDef.getString("isUnique");
    if (!"N".equalsIgnoreCase(isUnique))
        indexObj.indexType = "AK";   //Unique keys
    else
        indexObj.indexType = "I";
    indexObj.persist(psIndexes);
}

void fillIndexColumnInfo(ownerModel, tableName, indexName, indexColDef, psIndexColumns) {
    def indexColObj              = new BicsIndexColumn();
    indexColObj.ownerModel       = ownerModel;
    indexColObj.tableName        = tableName;
    indexColObj.indexName        = indexName;
    indexColObj.indexColumnName  = indexColDef.getString("columnName");
    indexColObj.indexColPosition = indexColDef.getInt("postn");
    indexColObj.persist(psIndexColumns);
}


