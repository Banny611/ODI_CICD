def isReversingTables = "T".equals("<% out.print(physicalSchemaType); %>");

if(isReversingTables) {
    metadataOpMain  = "odi_bics_list_tables";
    metadataOp      = "odi_bics_get_table_metadata";
    requestTemplate = "tablename";
}

def metadataFileMain = "<% out.print(dataDirectory); %>" + metadataOpMain + "." + java.util.UUID.randomUUID().toString();

def filterTables      = "<%=odiRef.getModel("REV_OBJ_PATT")%>".replace("%", ".*");

if (filterTables == null)
  filterTables = ".*";

def removeAliasChars  = "<%=odiRef.getModel("REV_ALIAS_LTRIM")%>";

Connection repoCx     = null;

try {

  ByteArrayOutputStream responseStreamMain = new ByteArrayOutputStream(256);

  invokeRESTReverse(metadataOpMain, "", "", responseStreamMain);
  
  //Obatin a list of tables/datasets 
  tableName = "";
  def tables = new JSONArray(responseStreamMain.toString("UTF-8"));

  repoCx = odiRef.getJDBCConnection("DEST");
  repoCx.setAutoCommit(true);

  def sqlTables = "insert into SNP_REV_TABLE (MOD_GUID,TABLE_NAME,RES_NAME,TABLE_ALIAS,TABLE_DESC_C,IND_SHOW,TABLE_TYPE) values (?,?,?,?,?,?,?)";
  def psTable = repoCx.prepareStatement(sqlTables);

  def sqlColumns = "insert into SNP_REV_COL (MOD_GUID,TABLE_NAME,COL_NAME,COL_HEADING,COL_DESC_C,DT_DRIVER,POS,LONGC,SCALEC,BYTES,COL_MANDATORY,CHECK_FLOW,CHECK_STAT,COL_DESC, DEF_VALUE) values (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)";
  PreparedStatement  psColumns = repoCx.prepareStatement(sqlColumns);

  def sqlIndexes = "insert into SNP_REV_KEY (MOD_GUID,TABLE_NAME,KEY_NAME,CONS_TYPE,IND_ACTIVE,CHECK_FLOW,CHECK_STAT) values (?,?,?,?,'1','1','1')";
  PreparedStatement  psIndexes = repoCx.prepareStatement(sqlIndexes);

  def sqlIndexColumns = "insert into SNP_REV_KEY_COL (MOD_GUID,TABLE_NAME,KEY_NAME,COL_NAME,POS) values (?,?,?,?,?)";
  PreparedStatement  psIndexColumns = repoCx.prepareStatement(sqlIndexColumns);

  def bicsDomain = odiRef.getInfo("SRC_CONNECT_BICS_ID_DOMAIN");
  
  for (int i = 0; i < tables.length(); i++) {
      tableName = tables.getString(i);

      if (!tableName.matches(filterTables))
          continue;
      else {
          fillContainer(iMod, tableName, removeAliasChars, psTable);
          // Now fetch metadata for the table
          ByteArrayOutputStream responseStream = new ByteArrayOutputStream(256);
          invokeRESTReverse(metadataOp, requestTemplate, tableName, responseStream);
          
          def rootMetadata = new JSONObject(responseStream.toString("UTF-8"));
          def columns = rootMetadata.get("columns");

          numColumns = columns.length();
          colPosition = 0;
          for(int j = 0; j < numColumns; j++) {
              colPosition += 1;
              def columnDef = columns.get(j);
              fillColumnInfo(iMod, tableName, columnDef, colPosition, isReversingTables, psColumns);
           }
          if(isReversingTables) {
              //handle indexes
              if ( "<%=odiRef.getOption("GET_TABLE_INDEXES")%>".equals("0") )
                  continue;
              
              def indexes = rootMetadata.get("indexes");
              int numIndexes = indexes.length();
              for(int j = 0; j < numIndexes; j++) {

                  def indexDef = indexes.get(j);
                  def indexName = indexDef.getString("indexName");
                  fillIndexInfo(iMod, tableName, indexDef, psIndexes);
                  
                  //add index columns
                  def indexColumns = indexDef.get("columns");
                  int numIndexColumns = indexColumns.length();
                  for(int k = 0; k < numIndexColumns; k++) {

                      def indexColDef = indexColumns.get(k);
                      def indexColumnName = indexColDef.getString("columnName");
                      fillIndexColumnInfo(iMod, tableName, indexName, indexColDef, psIndexColumns);
      
                  }
              }
          }
      }
  }
  psTable.close();
  psColumns.close();
  psIndexes.close();
  psIndexColumns.close();
} catch (Exception e) {
  logInfo("Failed with exception.");
  logInfo(e.toString());
  throw e;
}

repoCx.commit();

if (repoCx!=null && !repoCx.isClosed()) 
  repoCx.close();]]></Field>
